                                <materialDesign:PackIcon Kind="ViewHeadline" Style="{DynamicResource iconMenu2}" />
                                <TextBlock Style="{DynamicResource textBlockMenu2}">Стали перегородки</TextBlock>
                            </StackPanel>
                        </Button>
                        <Separator/>
                        <Button Name="btn_p1050" Margin="3" Click="btnMenu_Click">
